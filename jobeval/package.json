{
  "name": "jobeval",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "data:download": "node scripts/download-bls-data.js",
    "data:process": "node scripts/process-bls-data.js",
    "data:setup": "npm run data:download && npm run data:process",
    "data:fetch-api": "node scripts/fetch-bls-data.js",
    "data:fetch-api:reset": "node scripts/fetch-bls-data.js --reset",
    "data:fetch-api:test": "node scripts/fetch-bls-data.js --geography=national --limit=10",
    "onet:download": "node scripts/download-onet-data.js",
    "onet:download:force": "node scripts/download-onet-data.js --force",
    "onet:verify": "node scripts/download-onet-data.js --verify",
    "prepare": "husky"
  },
  "dependencies": {
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.553.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.10.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "xlsx": "^0.18.5"
  },
  "lint-staged": {
    "src/lib/persistence/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/features/data-management/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx,css,json}": [
      "prettier --write"
    ]
  }
}
